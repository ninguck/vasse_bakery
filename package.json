{
  "name": "vasse_bakery",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "tailwind:debug": "npx tailwindcss -i ./src/app/globals.css -o debug-output.css --watch",
    "test": "dotenv -e .env.test -- jest --runInBand",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:api": "jest --testPathPattern=src/__tests__/api",
    "test:validation": "jest --testPathPattern=validation --verbose --coverage",
    "db:push:test": "cross-env DOTENV_CONFIG_PATH=.env.test npx dotenv -e .env.test -- prisma db push",
    "db:push:prod": "prisma db push"
  },
  "dependencies": {
    "@prisma/client": "^6.10.1",
    "@radix-ui/react-accordion": "^1.2.11",
    "@radix-ui/react-dialog": "^1.1.14",
    "@radix-ui/react-hover-card": "^1.1.14",
    "@radix-ui/react-slot": "^1.2.3",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "embla-carousel-react": "^8.6.0",
    "framer-motion": "^12.18.1",
    "lucide-react": "^0.516.0",
    "next": "15.3.3",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-hot-toast": "^2.5.2",
    "tailwind-merge": "^3.3.1",
    "tailwindcss-animate": "^1.0.7",
    "zod": "^3.25.74"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@types/jest": "^30.0.0",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "autoprefixer": "^10.4.21",
    "cross-env": "^7.0.3",
    "dotenv-cli": "^8.0.0",
    "eslint": "^9",
    "eslint-config-next": "15.3.3",
    "jest": "^30.0.3",
    "postcss": "^8.5.6",
    "prisma": "^6.10.1",
    "tailwindcss": "3.4.0",
    "ts-jest": "^29.4.0",
    "tw-animate-css": "^1.3.4",
    "typescript": "^5"
  }
}
